<% content_for :head do %>  
<%= javascript_include_tag 'jquery.dataTables','highcharts' %>  
<%= stylesheet_link_tag 'demo_table_jui' %>
<script type="text/javascript">
$(document).ready(function() {

	$('#procesos').dataTable({
	 "bJQueryUI": true,
		"aoColumns": [
			null,
            null,
			null,
			null,
			null
        ]
                   
	});
});
</script>
 <% end %>
<h1>Lista de procesos</h1>
<%= link_to 'New proceso', new_proceso_path %>

<table id="procesos">
	<thead>
  		<tr>
	   	 	<th>Name</th>
		    <th>Software</th>
			<th></th>
			<th></th>
			<th></th>
		</tr>
	</thead>
	<tbody>
<% @procesos.each do |proceso| %>
  <tr>
    <td><%=h proceso.name %></td>
    <td><%=h proceso.software.name %></td>
    <td><%= link_to 'Show', proceso %></td>
    <td><%= link_to 'Edit', edit_proceso_path(proceso) %></td>
    <td><%= link_to 'Destroy', proceso, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</tbody>
</table>



