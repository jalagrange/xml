<script type="text/javascript" charset="utf-8">
	$(function() {
		new Highcharts.Chart({
			chart: {
				renderTo: "<%= insert_div %>",
				defaultSeriesType: 'bar'
			},
			title: {
				text: '<%= @chart.title %>'
			},
			xAxis:{
				categories: ['Grupos']

			},
			yAxis:{
				title:{
					text: "Prueba"
				}
			},
			tooltip: {
			         formatter: function() {
			            return ''+
			                this.series.name +': '+ this.y	;
			         }
			      },
			      plotOptions: {
			         bar: {
			            dataLabels: {
			               enabled: true
			            }
			         }
			      },
			legend: {
			         layout: 'vertical',
			         align: 'right',
			         verticalAlign: 'top',
			         x: -90,
			         y: 20,
			         borderWidth: 1,
			         backgroundColor: '#FFFFFF'
			      },
			
				series: [

				<% for series in @data %>
				{	
					name:<%= "'#{series[:name]}'"%>,
					data: [
						<% for datapoint in series[:data] %>
				         <%= "#{datapoint.data_attribute}," %>
						<% end %>

				         		]
				},										
				<% end %>
				]
			});
		});
</script>