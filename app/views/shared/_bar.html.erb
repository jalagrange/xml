<script type="text/javascript" charset="utf-8">
	$(function() {
		new Highcharts.Chart({
			chart: {
				renderTo: "<%= insert_div %>",
				defaultSeriesType: 'bar'
			},
			title: {
				text: '<%= @chart.title %>'
			},
			xAxis:{
				categories: ['Grupos']

			},
			yAxis:{
				title:{
					text: "Prueba"
				}
			},
			tooltip: {
			         formatter: function() {
			            return ''+
			                this.series.name +': '+ this.y	;
			         }
			      },
			      plotOptions: {
			         bar: {
			            dataLabels: {
			               enabled: true
			            }
			         }
			      },
			legend: {
			         layout: 'vertical',
			         align: 'right',
			         verticalAlign: 'top',
			         x: 0,
			         y: 20,
			         borderWidth: 1,
			         backgroundColor: '#FFFFFF'
			      },
			
				series: [

				<% for series in @data %>
				<% for datapoint in series[:data] %>
				<% if ! (datapoint.data_attribute.to_i == 0) %>
				{	
					name:<%= "'#{series[:name]}'"%>,
					data: [
						
							
				         		<%= "#{datapoint.data_attribute}" %>
							
						

				         		]
				},
				<% end %>
				<% end %>										
				<% end %>
				]
			});
		});
</script>