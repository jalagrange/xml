<% content_for :head do %>  
<%= javascript_include_tag 'jquery.dataTables','highcharts' %>  
<%= stylesheet_link_tag 'demo_table_jui' %>
<script type="text/javascript">
$(document).ready(function() {

	$('#jobs').dataTable({
	 "bJQueryUI": true,
		"aoColumns": [
			null,
            null,
			null
        ]
                   
	});
});
</script>
<% end %>


			<table id="jobs" title="jobs" summary="Tiene los trabajos ejecutados en PBS">
			<thead>
				<tr>
					<th>Projecto</th>
					<th>Software</th>
					<th>Proceso</th>					
				</tr>		
			</thead>
			<tbody>
					<% for job in @jobs %>
					<tr>
							<td><%= job.project %></td>
							<td><%= job.proceso.software.name %></td>
							<td><%= job.proceso.name %></td>										
					</tr>
					<% end %>
			</tbody>
		</table>

		
		
		