<script type="text/javascript" charset="utf-8">
	$(function() {
		new Highcharts.Chart({
			chart: {
				renderTo: "<%= div_bar %>",
				defaultSeriesType: 'bar'
			},
			title: {
				text: "Walltime"
			},
			xAxis:{
				categories: ['Grupos']

			},
			yAxis:{
				title:{
					text: "Prueba"
				}
			},
			tooltip: {
			         formatter: function() {
			            return ''+
			                this.series.name +': '+ this.y	;
			         }
			      },
			      plotOptions: {
			         bar: {
			            dataLabels: {
			               enabled: true
			            }
			         }
			      },
			legend: {
			         layout: 'vertical',
			         align: 'right',
			         verticalAlign: 'top',
			         x: -90,
			         y: 20,
			         borderWidth: 1,
			         backgroundColor: '#FFFFFF'
			      },
			
		 series: [
		<% for job in @daily_jobs %>
						{
						name:'<%= job.day %>',
						data:[<%= job.total_walltime %>]
								},										
								<% end %>
					      ]
			});
		});
</script>