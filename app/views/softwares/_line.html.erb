<script type="text/javascript" charset="utf-8">
$(document).ready(function() {
   chart = new Highcharts.Chart({
      chart: {
         renderTo: '<%= div_line %>',
         defaultSeriesType: 'line',
         marginRight: 130,
         marginBottom: 25
      },
      title: {
         text: 'Walltime',
         x: -20 //center
      },
     
      xAxis: {type: 'datetime' ,
         categories: [	<% for job in @jobs_diarios %>
			'<%= job.day %>', 
			<% end %>]
      },
      yAxis: {
         title: {
            text: 'Tiempo en segundos'
         },
         plotLines: [{
            value: 0,
            width: 1,
            color: '#808080'
         }]
      },
      tooltip: {
         formatter: function() {
                   return '<b>'+ this.series.name +'</b><br/>'+
               this.x +': '+ this.y +' segundos';
         }
      },
      legend: {
         layout: 'vertical',
         align: 'right',
         verticalAlign: 'top',
         x: -10,
         y: 100,
         borderWidth: 0
      },
	series: [
	{	pointInterval: <%= 1.day * 1000 %>,
		pointStart: <%= 2.weeks.ago.at_midnight.to_i * 1000 %>, 
		name:'Waltime',
		data:[
		<% for job in @jobs_diarios %>
		[<%= job.day %>,<%= job.walltime %> ],
		<% end %>
		]
	},										

	]

   });
   
   
});
</script>